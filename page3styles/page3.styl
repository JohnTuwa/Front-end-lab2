@import "../globalVars.styl"

table
  margin-left: auto;
  margin-right: auto;
  border-collapse: collapse;

img
  width: 100%;
  height: auto;

.two-images
  display: flex;
  justify-content: space-between;

.two-images img
  width: 48%;
  height: auto;

.main-table
  width: 100%;
  max-width: maxW;
  border: stBorder;

.main-table th
  border: stBorder;

.main-table td
  padding: 10px;
  vertical-align: top;

.main-cols
  width: 40%;

.secondary-table
  font-size: 10px;

.third-col
  border-left: stBorder;
